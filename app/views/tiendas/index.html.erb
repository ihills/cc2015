<h1>Listing tiendas</h1>

<table>
  <thead>
    <tr>
      <th>Id tienda</th>
      <th>Nombre</th>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Sitio</th>
      <th>Correo</th>
      <th>Telefono</th>
      <th>Logo</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tiendas.each do |tienda| %>
      <tr>
        <td><%= tienda.id_tienda %></td>
        <td><%= tienda.nombre %></td>
        <td><%= tienda.facebook %></td>
        <td><%= tienda.twitter %></td>
        <td><%= tienda.sitio %></td>
        <td><%= tienda.correo %></td>
        <td><%= tienda.telefono %></td>
        <td><%= tienda.logo %></td>
        <td><%= tienda.descripcion %></td>
        <td><%= link_to 'Show', tienda %></td>
        <td><%= link_to 'Edit', edit_tienda_path(tienda) %></td>
        <td><%= link_to 'Destroy', tienda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tienda', new_tienda_path %>
