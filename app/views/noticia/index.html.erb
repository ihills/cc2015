<h1>Listing noticia</h1>

<table>
  <thead>
    <tr>
      <th>Id noticia</th>
      <th>Fecha</th>
      <th>Titulo</th>
      <th>Subtitulo</th>
      <th>Foto</th>
      <th>Texto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @noticia.each do |noticium| %>
      <tr>
        <td><%= noticium.id_noticia %></td>
        <td><%= noticium.fecha %></td>
        <td><%= noticium.titulo %></td>
        <td><%= noticium.subtitulo %></td>
        <td><%= noticium.foto %></td>
        <td><%= noticium.texto %></td>
        <td><%= link_to 'Show', noticium %></td>
        <td><%= link_to 'Edit', edit_noticium_path(noticium) %></td>
        <td><%= link_to 'Destroy', noticium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Noticium', new_noticium_path %>
